{
  "name": "fyrts/chrome-devtools-protocol",
  "type": "library",
  "description": "Chrome Devtools Protocol client for PHP",
  "keywords": [
    "chrome",
    "chrome-devtools",
    "chrome-devtools-protocol",
    "headless-chrome",
    "web",
    "automation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Kulhan",
      "email": "jakub.kulhan@gmail.com"
    },
    {
      "name": "Sam Feyaerts",
      "email": "sam@feyaerts.me"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "php": "^7.4|^8.0",
    "symfony/filesystem": ">=5.3",
    "symfony/process": ">=4.4.30",
    "chrome-php/wrench": "^1.3"
  },
  "require-dev": {
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "nette/php-generator": "~2.1|~3.0",
    "phpstan/phpstan": "^1.4.3",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "ChromeDevtoolsProtocol\\": [
        "src/ChromeDevtoolsProtocol/",
        "gen-src/ChromeDevtoolsProtocol/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ChromeDevtoolsProtocol\\": [
        "test/ChromeDevtoolsProtocol/"
      ]
    }
  }
}
